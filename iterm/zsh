function ezsh() {
  vim ~/.zshrc
}

function rzsh() {
  source ~/.zshrc
}

function gs() {
  git status
}

function gc() {
  git add .
  git commit -m $1
}

function ga() {
  git add .
}

function gp() {
  git push
}

function gcp() {
  git add .
  git commit -m $1
  git push
}